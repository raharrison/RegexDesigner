<Window x:Class="RegexDesigner.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <DockPanel LastChildFill="True">
        <TabControl DockPanel.Dock="Right" Padding="5,0,5,0">
            <TabItem Header="Samples"></TabItem>
            <TabItem Header="Saved"></TabItem>
        </TabControl>
        <TabControl>
            <TabItem Header="Match"></TabItem>
            <TabItem Header="Replace"></TabItem>
        </TabControl>
    </DockPanel>
</Window>
